    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.10.0",
    "sequelize": "^6.29.3",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-fileupload": "^1.4.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.2",
    "@types/pg": "^8.6.6",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21",
    "winston": "^3.8.2"
